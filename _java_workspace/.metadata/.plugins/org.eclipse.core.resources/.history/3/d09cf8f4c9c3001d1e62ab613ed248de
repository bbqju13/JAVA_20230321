package day16;

public class Word {
	
	private String[] word;
	private String[] mean;
	private int wordCount;
	
	public Word() {}
	
	public Word(String[]word, String[]mean) {
		this.word=word;
		this.mean=mean;
		int wordCount=0;
	}

	public String[] getWord() {
		return word;
	}

	public void setWord(String[] word) {
		this.word = word;
	}

	public String[] getMean() {
		return mean;
	}

	public void setMean(String[] mean) {
		this.mean = mean;
	}
	

}
